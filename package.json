{
  "name": "kotohiro-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "LOCAL=1 && react-router dev",
    "build": "cross-env NODE_OPTIONS=\"-C workerd\" react-router build",
    "deploy": "bun run build && wrangler pages functions build --outdir build/worker && wrangler deploy",
    "format": "prettier --write ./app/**",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "storybook": "SB=1 storybook dev -p 6006",
    "storybook:build": "SB=1 storybook build",
    "typecheck": "react-router typegen && wrangler types && tsc -b",
    "icongen": "bunx @svgr/cli --out-dir app/components/Icons -- app/assets/icons && prettier --write app/components/Icons/**/*.tsx"
  },
  "dependencies": {
    "@conform-to/react": "^1.2.2",
    "@headlessui/react": "^2.2.0",
    "@lexical/react": "^0.27.2",
    "@react-router/cloudflare": "^7.1.1-pre.0",
    "@react-router/fs-routes": "^7.1.0",
    "@react-router/serve": "^7.1.3",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.0.6",
    "compressorjs": "^1.2.1",
    "conform-to-valibot": "^1.11.0",
    "dayjs": "^1.11.13",
    "isbot": "^4.1.0",
    "lexical": "^0.27.2",
    "motion": "^12.5.0",
    "openapi-fetch": "^0.12.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-icons": "^5.3.0",
    "react-router": "^7.0.0",
    "react-spring": "^9.7.4",
    "react-toastify": "^10.0.6",
    "react-use-gesture": "^9.1.3",
    "swiper": "^11.2.5",
    "tailwind-variants": "^0.2.1",
    "valibot": "^0.42.1",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241112.0",
    "@react-router/dev": "^7.1.3",
    "@storybook/addon-essentials": "^8.3.3",
    "@storybook/addon-interactions": "^8.3.3",
    "@storybook/addon-links": "^8.3.3",
    "@storybook/react": "^8.3.3",
    "@storybook/react-vite": "^8.3.3",
    "@svgr/cli": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/bun": "^1.1.11",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.9.0",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "storybook": "^8.3.3",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^3.87.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
