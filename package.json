{
  "name": "kotohiro-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "build": "react-router build",
    "deploy": "CF_ENV=develop bun run build && wrangler pages functions build --outdir build/worker && wrangler deploy --env develop",
    "format": "bunx biome format --write ./app",
    "lint": "biome check .",
    "deps": "bun depcruise --config .dependency-cruiser.cjs .",
    "storybook": "SB=1 storybook dev -p 6006",
    "storybook:build": "SB=1 storybook build",
    "typecheck": "react-router typegen && wrangler types && tsc -b",
    "icongen": "bunx @svgr/cli --out-dir app/components/Icons -- app/assets/icons && bunx biome format --write ./app/components/Icons"
  },
  "dependencies": {
    "@conform-to/react": "^1.2.2",
    "@conform-to/valibot": "^1.6.1",
    "@react-router/cloudflare": "^7.1.1-pre.0",
    "@react-router/fs-routes": "^7.1.0",
    "@react-router/serve": "^7.1.3",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.0.6",
    "@tiptap/core": "^2.11.7",
    "@tiptap/extension-dropcursor": "^2.11.7",
    "@tiptap/extension-image": "^2.11.7",
    "@tiptap/extension-link": "^2.11.7",
    "@tiptap/extension-placeholder": "^2.11.7",
    "@tiptap/pm": "^2.11.7",
    "@tiptap/react": "^2.11.7",
    "@tiptap/starter-kit": "^2.11.7",
    "chart.js": "^4.4.8",
    "compressorjs": "^1.2.1",
    "conform-to-valibot": "^1.11.0",
    "dayjs": "^1.11.13",
    "gsap": "^3.13.0",
    "isbot": "^4.1.0",
    "motion": "^12.5.0",
    "openapi-fetch": "^0.12.2",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "react-router": "^7.0.0",
    "react-toastify": "^10.0.6",
    "react-use-gesture": "^9.1.3",
    "simplebar-react": "^3.3.0",
    "swiper": "^11.2.5",
    "tailwind-variants": "^0.2.1",
    "tiptap-extension-resize-image": "^1.2.1",
    "valibot": "^0.42.1",
    "xss": "^1.0.15",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@cloudflare/workers-types": "^4.20241112.0",
    "@react-router/dev": "^7.1.3",
    "@storybook/addon-essentials": "^8.3.3",
    "@storybook/addon-interactions": "^8.3.3",
    "@storybook/addon-links": "^8.3.3",
    "@storybook/react": "^8.3.3",
    "@storybook/react-vite": "^8.3.3",
    "@svgr/cli": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/bun": "^1.1.11",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "cross-env": "^7.0.3",
    "dependency-cruiser": "^16.10.1",
    "openapi-typescript": "^7.6.1",
    "postcss": "^8.4.49",
    "rollup-plugin-visualizer": "^5.14.0",
    "storybook": "^8.3.3",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "4"
  },
  "svgr": {
    "plugins": [
      "@svgr/plugin-svgo",
      "@svgr/plugin-jsx"
    ],
    "jsxRuntime": "automatic",
    "noDimensions": true,
    "typescript": true,
    "prettier": false,
    "svgoConfig": {
      "plugins": [
        {
          "name": "preset-default",
          "params": {
            "overrides": {
              "removeViewBox": false,
              "cleanupIds": {
                "remove": true,
                "minify": false
              }
            }
          }
        }
      ]
    }
  },
  "patchedDependencies": {
    "dependency-cruiser@16.10.1": "patches/dependency-cruiser@16.10.1.patch"
  }
}